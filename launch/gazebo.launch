<launch>
  <param command="$(find xacro)/xacro $(find pleiades_urdf_description)/urdf/pleiades_urdf.xacro" name="robot_description"/>
  <node args="-param robot_description -urdf -model pleiades_urdf" name="spawn_urdf" pkg="gazebo_ros" type="spawn_model"/>
  <include file="$(find gazebo_ros)/launch/empty_world.launch">
   <arg name="paused" value="false"/>
    <arg name="use_sim_time" value="true"/>
    <arg name="gui" value="true"/>
    <arg name="headless" value="false"/>
    <arg name="debug" value="false"/>
  <!--clude file="$(find pleiades_urdf_description)/launch/simple_world.launch">-->
   
  </include>
</launch>
